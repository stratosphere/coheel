# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8

# 4 space indentation
[*.java]
indent_style = tab
indent_size = 4

[*.scala]
indent_style = tab
indent_size = 4

[pom.xml]
indent_style = tab
indent_size = 4
